{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**",
      "use": "@vercel/static"
    },
    {
      "src": "GIF/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/pages/main.html"
    },
    {
      "src": "/titles",
      "dest": "/titles"
    },
    {
      "src": "/courses",
      "dest": "/courses"
    },
    {
      "src": "/presenters",
      "dest": "/presenters"
    }
  ]
}