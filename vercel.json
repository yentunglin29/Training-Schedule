{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**",
      "use": "@vercel/static"
    },
    {
      "src": "GIF/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/pages/main.html"
    },
    {
      "src": "/api/titles",
      "dest": "/api/app.js"
    },
    {
      "src": "/api/courses",
      "dest": "/api/app.js"
    },
    {
      "src": "/api/presenters",
      "dest": "/api/app.js"
    }
  ]
}
